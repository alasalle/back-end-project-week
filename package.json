    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^2.4.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2"
  }
